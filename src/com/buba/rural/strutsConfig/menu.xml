<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
   	<package name="menu" namespace="/menu" extends="struts-default">
   		<!-- 分页查询菜单 -->
   		<action name="findMenu" class="menuAction" method="findMenu"></action>
   		<action name="save" class="menuAction" method="saveMenu"></action>
   		<action name="insertMenu" class="menuAction" method="insertMenu"></action>
   		<action name="deleteMenu" class="menuAction" method="deleteMenu"></action>
   		<action name="findMenuTree" class="menuAction" method="findMenuTree"></action>
   	</package>
</struts>